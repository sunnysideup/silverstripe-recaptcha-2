{
    "name": "kmedia/silverstripe-recaptcha",
    "description": "A spam protector extension using Google's reCAPTCHA 2",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "extension",
        "spamprotection"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Sebastian Kalicki",
            "email": "s.kalicki@kmedia.email",
            "homepage": "http://kmedia.rocks",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/kMediaGbR/silverstripe-recaptcha/issues"
    },
    "require": {
        "ext-curl": "*",
        "ext-json": "*",
        "silverstripe/framework": "^4.5 || ^5.0",
        "silverstripe/spamprotection": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Kmedia\\ReCaptcha\\": "src/"
        }
    },
    "extra": {
        "expose": [
            "javascript"
        ]
    }
}

